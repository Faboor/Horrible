        <item name="colorAccent">@color/colorAccent</item>
        <item name="fabStyle">@style/Theme.Fab</item>
        <item name="tabTextColor">#85ffffff</item>
        <item name="colorItem">#18ffffff</item>

        <item name="recyclerAccent2">#a8ffffff</item>
